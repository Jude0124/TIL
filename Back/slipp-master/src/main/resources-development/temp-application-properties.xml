    <entry key="environment">DEVELOPMENT</entry>

    <!-- net.slipp.service.qna.FacebookService.java -->
    <entry key="facebook.application.url">test</entry>

    <!-- slipp-servlet.xml -->
    <entry key="application.url">test</entry>
    <entry key="resources.cache.seconds">300</entry>


